{
  "name": "scrollpanels",
  "version": "1.0.0",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "description": "An exploration of smooth panel scroll effects based on Margot Priolet's website.",
  "scripts": {
    "start": "parcel index.html",
    "clean": "rm -rf dist && mkdir dist",
    "clean-dev": "rmdir /s /q \"dist\" && mkdir \"dist\"",
    "build:parcel": "parcel build index.html --no-content-hash --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RiddhimanRaguraman/portfolio"
  },
  "keywords": [],
  "author": "Riddhiman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RiddhimanRaguraman/portfolio/issues"
  },
  "dependencies": {
    "@studio-freight/lenis": "^0.2.9",
    "gsap": "^3.11.3",
    "imagesloaded": "^5.0.0",
    "parcel": "^2.10.3"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "devDependencies": {
    "parcel-plugin-static-files-copy": "^1.2.4"
  }
}
